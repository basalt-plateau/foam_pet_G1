{
  "name": "sveltenetics",
  "version": "0.0.1",
  "scripts": {
    "dev-2": "vite",
    "foam": "vite dev --host 0.0.0.0 --port 21000 --clearScreen false",
    "foam-local": "vite dev --port 21000 --clearScreen false",
    "forget": "rm -rf /Metro/vehicles/foam_pet/sveltnetics_packets",
    "refresh_node_modules": "rm -rf node_modules && pnpm install",
    "build_frontend": "pnpm run forget && pnpm run refresh_node_modules && vite build",
    "build-2": "vite build",
	"build_rules": "pnpm run build_rules_full && pnpm run build_rules_legend",
	"build_rules_full": "npx generate-license-file",
	"build_rules_full----": ".glf.cjs has the config for this",
	"build_rules_legend": "pnpm licenses list > /Metro/vehicles/foam_pet/Rules/Laboratory/frontend_rules_legend.txt",
    "health": "node --version && vitest",
    "health----": "if node is not installed, vitest doesn't do anything..",
    "preview": "vite preview",
    "lint": "eslint .",
    "showbiz": "python3 -m http.server 21000 --directory build"
  },
